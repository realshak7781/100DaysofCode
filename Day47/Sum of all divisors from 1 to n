//Sum of all divisors from 1 to n





//T.C: O(n)
//S.C: O(1)

//QUESTION LNK: https://www.geeksforgeeks.org/problems/sum-of-all-divisors-from-1-to-n4738/1

USE OF KNOWLEDGE OF  BASICS


class Solution
{
public:
    long long sumOfDivisors(int N)
    {
        long long sum=0;
        for(int i=1;i<=N;i++){
            long long multpl=N/i;
            sum+=i*(multpl);
        }
        
        return sum;
    }
   
};
